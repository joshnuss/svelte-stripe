{
  "name": "svelte-stripe",
  "version": "0.0.0-development",
  "homepage": "https://sveltestripe.com",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -c development -- vite dev",
    "build": "dotenv -c development vite build",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "preview": "dotenv -c development vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
    "format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@stripe/stripe-js": "^5.2.0",
    "@sveltejs/adapter-vercel": "^5.3.0",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-svelte": "^2.45.1",
    "mdsvex": "^0.11.0",
    "prettier": "~3.2.5",
    "prettier-plugin-svelte": "^3.2.6",
    "prismjs": "^1.29.0",
    "publint": "^0.2.12",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "stripe": "^17",
    "svelte": "^5.0.0",
    "svelte-check": "^4.1.0",
    "svelte-eslint-parser": "^0.42.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.4"
  },
  "peerDependencies": {
    "@stripe/stripe-js": "^3 || ^4",
    "svelte": "^3 || ^4 || ^5"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshnuss/svelte-stripe.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "next",
      "svelte-5"
    ]
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ]
}
